class Tester {
	public static void main(String[] args) {
		// Implement your code here 
 int num1=9;
 int num2=4;
 int num3=11;


 if (num1>num2 && num1>num3) {
 System.out.println("The Greatest Number among these 3 numbers is" + num1);
 } else if (num2>num1 && num2>num3) {
 System.out.println("The Greatest Number among these 3 numbers is" + num2);
 } else if (num3>num1 && num3>num2) {
 System.out.println("The Greatest Number among these 3 numbers is" + num3);
 }
	}
}

